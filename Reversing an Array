#include<stdio.h>
void reverseArray(int a[],int n){
	int temp;
	for(int i=0;i<=(n/2);i++){
		int temp=a[i];
		a[i]=a[n-i-1];
		a[n-i-1]=temp;
	}
	for(int i=0;i<n;i++){
		printf("%d",a[i]);
	}
}
main(){
	int n;
	printf("Enter length of array:");
	scanf("%d",&n);
	int arr[n];
	for(int i=0;i<n;i++){
		printf("Enter %d element:",i+1);
		scanf("%d",&arr[i]);
	}
	reverseArray(arr,n);
}

#include<stdio.h>
void sumOfEvenIndices(int a[],int n){
	int sum=0;
	for(int i=0;i<n;i+=2){
		sum+=a[i];
	}
	printf("%d",sum);
}
main(){
	int n;
	printf("Enter length of array:");
	scanf("%d",&n);
	int arr[n];
	for(int i=0;i<n;i++){
		printf("Enter %d element:",i+1);
		scanf("%d",&arr[i]);
	}
	sumOfEvenIndices(arr,n);
}

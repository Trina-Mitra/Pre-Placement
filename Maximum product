#include <stdio.h>
int maxProduct(int arr[], int n) {
    int maxProduct = arr[0] * arr[1] * arr[2];
    for (int i = 0; i < n - 2; i++) {
        for (int j = i + 1; j < n - 1; j++) {
            for (int k = j + 1; k < n; k++) {
                int product = arr[i] * arr[j] * arr[k];
                if (product > maxProduct) {
                    maxProduct = product;
                }
            }
        }
    }
    return maxProduct;
}
int main() {
    int arr[] = {10, 3, 5, 6, 20};
    int n = sizeof(arr) / sizeof(arr[0]);
    printf("%d\n", maxProduct(arr, n));
    return 0;
}

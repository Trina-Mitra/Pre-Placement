#include <stdio.h>
#define MAX 100
void rotateAnticlockwise90(int matrix[MAX][MAX], int n) {
    for(int i = 0; i < n; i++) {
        for(int j = i + 1; j < n; j++) {
            int temp = matrix[i][j];
            matrix[i][j] = matrix[j][i];
            matrix[j][i] = temp;
        }
    }
    for(int j = 0; j < n; j++) {
        for(int i = 0, k = n - 1; i < k; i++, k--) {
            int temp = matrix[i][j];
            matrix[i][j] = matrix[k][j];
            matrix[k][j] = temp;
        }
    }
}
void printMatrix(int matrix[MAX][MAX], int n) {
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < n; j++) {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }
}
int main() {
    int n;
    int matrix[MAX][MAX];
    printf("Enter the size (n) of the square matrix: ");
    scanf("%d", &n);
    printf("Enter the matrix elements:\n");
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < n; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
    rotateAnticlockwise90(matrix, n);
    printf("Matrix after 90-degree anticlockwise rotation:\n");
    printMatrix(matrix, n);
    return 0;
}

